{
  "name": "ofi",
  "version": "1.3.4",
  "description": "Yet another arguments parser",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": "mrozio13pl/ofi",
  "author": "mrozio13pl",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup src/index.ts --clean --dts --minify --format esm,cjs && node scripts/size.mjs",
    "test": "cross-env NODE_OPTIONS=\"-r ts-node/register\" uvu test .test.js$",
    "lint": "npx publint && eslint .",
    "publish": "clean-publish"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "args",
    "parser",
    "options",
    "command",
    "cmd",
    "yargs",
    "commander",
    "sade",
    "opt",
    "argv"
  ],
  "devDependencies": {
    "@mrozio/eslint-config": "^1.0.4",
    "@types/node": "^20",
    "clean-publish": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "fast-folder-size": "^2.2.0",
    "pretty-bytes": "^6.1.1",
    "publint": "^0.2.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "uvu": "^0.5.6"
  },
  "packageManager": "^pnpm@8.9.0"
}
